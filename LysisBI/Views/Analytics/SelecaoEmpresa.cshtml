@model LysisBI.Models.UsuarioModel



<html>
<head>
    <title></title>
    <meta charset="utf-8" />

</head>
<body>




    <b>&nbsp;Selecione a Empresa</b>
    <br />


    @using (Html.BeginForm("SelecaoEmpresa", "Analytics", FormMethod.Post))
    {



        for (int i = 0; i < Model.ListaDeEmpresa.Count(); i++)
        {
            @Html.CheckBoxFor(m => m.ListaDeEmpresa[i].Selected, new { @class = "checkSelecaoUnica" })

            @Model.ListaDeEmpresa[i].Nome

            <br />
            @Html.HiddenFor(m => m.ListaDeEmpresa[i].Nome)
            @Html.HiddenFor(m => m.ListaDeEmpresa[i].Isn_Pessoa)

        }


        <b><u><a href="#" class="submitFormWithoutReload">Selecionar</a></u></b>

    }


    <!-- Scripts     -->
    <script src="~/scripts/funcoes.js"></script>
</body>

</html>








